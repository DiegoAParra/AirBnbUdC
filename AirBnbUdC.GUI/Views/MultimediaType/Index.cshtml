@model IEnumerable<AirBnbUdC.GUI.Models.Parameters.MultimediaTypeModel>

@{
    ViewBag.Title = "Index";
}

<h2>Tipo de multimedias</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create", null, new { @Class = "btn btn-primary mt-5 mb-3" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MultimediaTypeName)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }, new { @Class = "btn btn-secondary" }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }, new { @Class = "btn btn-info" }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id }, new { @Class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
